{
   "name": "graphql-server-boilerplate",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "devDependencies": {
      "@types/bcryptjs": "^2.4.2",
      "@types/connect-redis": "^0.0.11",
      "@types/express-session": "^1.15.14",
      "@types/ioredis": "^4.0.15",
      "@types/jest": "^24.0.17",
      "@types/node": "^12.7.2",
      "@types/uuid": "^3.4.5",
      "gql2ts": "^1.10.1",
      "jest": "^24.9.0",
      "nodemon": "^1.19.1",
      "prettier": "^1.18.2",
      "ts-jest": "^24.0.2",
      "ts-node": "8.3.0",
      "tslint": "^5.18.0",
      "tslint-config-prettier": "^1.18.0",
      "typescript": "3.5.3"
   },
   "dependencies": {
      "@gql2ts/from-schema": "^1.10.1",
      "@types/sparkpost": "^2.1.4",
      "@types/yup": "^0.26.23",
      "bcryptjs": "^2.4.3",
      "connect-redis": "^3.4.2",
      "express": "^4.17.1",
      "express-session": "^1.16.2",
      "graphql": "^14.4.2",
      "graphql-import": "^0.7.1",
      "graphql-request": "^1.8.2",
      "graphql-tools": "^4.0.5",
      "graphql-yoga": "^1.18.2",
      "ioredis": "^4.14.0",
      "pg": "^7.12.1",
      "reflect-metadata": "^0.1.13",
      "sparkpost": "^2.1.3",
      "typeorm": "0.2.18",
      "uuid": "^3.3.3",
      "yup": "^0.27.0"
   },
   "scripts": {
      "start": "NODE_ENV=development nodemon --exec TS_NODE_FILES=true ts-node src/index.ts",
      "gen-graphql-types": "TS_NODE_FILES=true ts-node ./scripts/gen-schema.ts",
      "test": "TS_NODE_FILES=true NODE_ENV=test jest --watch"
   },
   "jest": {
      "globalSetup": "./src/test-setup/callSetup.js",
      "globalTeardown": "./src/test-setup/callTearDown.js",
      "transform": {
         "^.+\\.tsx?$": "ts-jest"
      },
      "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
      "moduleFileExtensions": [
         "ts",
         "tsx",
         "js",
         "jsx",
         "json",
         "node"
      ]
   }
}
